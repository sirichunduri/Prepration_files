1 . SELECT column1, column2, ...FROM table_name;
2 . SELECT DISTINCT column1, column2, ...FROM table_name;
     eg : SELECT COUNT(DISTINCT Country) FROM Customers;
     eg : SELECT Count(*) AS DistinctCountries FROM (SELECT DISTINCT Country FROM Customers);
3 . SELECT column1, column2, ...FROM table_name WHERE condition;
4 . SELECT column1, column2, ...FROM table_name WHERE condition1 AND condition2 AND condition3 ...;
   eg : SELECT * FROM Customers WHERE Country='Germany' AND (City='Berlin' OR City='MÃ¼nchen');
   eg : SELECT * FROM Customers WHERE NOT Country='Germany' AND NOT Country='USA';
5 . SELECT column1, column2, ...FROM table_name ORDER BY column1, column2, ... ASC|DESC;
   eg : SELECT * FROM Customers ORDER BY Country ASC, CustomerName DESC;
6 : INSERT INTO table_name (column1, column2, column3, ...) VALUES (value1, value2, value3, ...);
   eg : INSERT INTO Customers (CustomerName, ContactName, Address, City, PostalCode, Country) VALUES ('Cardinal', 'Tom B. Erichsen', 'Skagen 21', 'Stavanger', '4006', 'Norway');
7 : SELECT column_names FROM table_name WHERE column_name IS NULL;
   eg : SELECT CustomerName, ContactName, Address FROM Customers WHERE Address IS NULL;
8 : SELECT column_names FROM table_name WHERE column_name IS NOT NULL; 
   eg : SELECT CustomerName, ContactName, Address FROM Customers WHERE Address IS NOT NULL;  
9 : UPDATE table_name SET column1 = value1, column2 = value2, ... WHERE condition;
   eg : UPDATE Customers SET ContactName = 'Alfred Schmidt', City= 'Frankfurt' WHERE CustomerID = 1;
10 : DELETE FROM table_name WHERE condition;
   eg : DELETE FROM Customers WHERE CustomerName='Alfreds Futterkiste';
11 : SELECT MIN(column_name) FROM table_name WHERE condition;
   eg : SELECT MIN(Price) AS SmallestPrice FROM Products;
12 : SELECT MAX(column_name) FROM table_name WHERE condition;
   eg : SELECT MAX(Price) AS LargestPrice FROM Products; 
13 : SELECT COUNT(column_name) FROM table_name WHERE condition;
   eg : SELECT COUNT(ProductID) FROM Products;
14 : SELECT AVG(column_name) FROM table_name WHERE condition;
   eg : SELECT AVG(Price) FROM Products;
15 : SELECT SUM(column_name) FROM table_name WHERE condition;
   eg : SELECT SUM(Quantity) FROM OrderDetails;
16 : SELECT column1, column2, ... FROM table_name WHERE columnN LIKE pattern;
   eg : SELECT * FROM Customers WHERE CustomerName LIKE 'a%';
   eg : SELECT * FROM Customers WHERE CustomerName LIKE '%or%';
   eg : SELECT * FROM Customers WHERE CustomerName LIKE '_r%';
   eg : SELECT * FROM Customers WHERE CustomerName LIKE 'a__%';
   eg : SELECT * FROM Customers WHERE ContactName LIKE 'a%o';
   eg : SELECT * FROM Customers WHERE CustomerName NOT LIKE 'a%';
17 : SELECT column_name(s) FROM table_name WHERE column_name IN (value1, value2, ...);
   eg : SELECT * FROM Customers WHERE Country IN ('Germany', 'France', 'UK');
   eg : SELECT * FROM Customers WHERE Country NOT IN ('Germany', 'France', 'UK');
18 : SELECT column_name(s) FROM table_name WHERE column_name IN (SELECT STATEMENT);
   eg : SELECT * FROM Customers WHERE Country IN (SELECT Country FROM Suppliers);
19 : SELECT column_name(s) FROM table_name WHERE column_name BETWEEN value1 AND value2;
   eg : SELECT * FROM Products WHERE Price BETWEEN 10 AND 20;
   eg : SELECT * FROM Products WHERE Price NOT BETWEEN 10 AND 20;
   eg : SELECT * FROM Products WHERE Price BETWEEN 10 AND 20 AND NOT CategoryID IN (1,2,3);
   eg : SELECT * FROM Products WHERE ProductName BETWEEN 'Carnarvon Tigers' AND 'Mozzarella di Giovanni' ORDER BY ProductName;
   eg : SELECT * FROM Products WHERE ProductName NOT BETWEEN 'Carnarvon Tigers' AND 'Mozzarella di Giovanni' ORDER BY ProductName;
   eg : SELECT * FROM Orders WHERE OrderDate BETWEEN #01/07/1996# AND #31/07/1996#;
   eg : SELECT * FROM Orders WHERE OrderDate BETWEEN '1996-07-01' AND '1996-07-31';
20 : SELECT column_name AS alias_name FROM table_name;
   eg : SELECT CustomerID AS ID, CustomerName AS Customer FROM Customers;
   eg : SELECT CustomerName AS Customer, ContactName AS [Contact Person] FROM Customers;
21 . SELECT Orders.OrderID, Customers.CustomerName, Orders.OrderDate FROM Orders INNER JOIN Customers ON Orders.CustomerID=Customers.CustomerID;
22 . SELECT column_name(s) FROM table1 LEFT JOIN table2 ON table1.column_name = table2.column_name;
23 . SELECT column_name(s) FROM table1 RIGHT JOIN table2 ON table1.column_name = table2.column_name;
24 . SELECT column_name(s) FROM table1 FULL OUTER JOIN table2 ON table1.column_name = table2.column_name WHERE condition;
25 . SELECT column_name(s) FROM table1 T1, table1 T2 WHERE condition;
26 . SELECT column_name(s) FROM table1 UNION SELECT column_name(s) FROM table2;
   eg : SELECT City FROM Customers UNION SELECT City FROM Suppliers ORDER BY City;
   